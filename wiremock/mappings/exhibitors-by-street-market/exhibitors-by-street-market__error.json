{
  "request": {
    "method": "GET",
    "urlPathPattern": "/(.+)/exhibitors",
    "queryParameters": {
      "page": {
        "equalTo": "1"
      },
      "perPage": {
        "equalTo": "10"
      }
    },
    "headers": {
      "Slug-Exhibitors": {
        "equalTo": "error"
      }
    }
  },
  "response": {
    "fixedDelayMilliseconds": 2000,
    "status": 500,
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
