{
  "request": {
    "method": "GET",
    "urlPathPattern": "/street-markets/(.+)",
    "queryParameters": {
      "page": {
        "equalTo": "1"
      },
      "perPage": {
        "equalTo": "10"
      },
      "weekday": {
        "equalTo": "tuesday"
      }
    }
  },
  "response": {
    "fixedDelayMilliseconds": 2000,
    "status": 200,
    "jsonBody": {
      "metadata": {
        "currentPage": 1,
        "itemsPerPage": 10,
        "totalPages": 1,
        "totalItems": 0,
        "isLastPage": true
      },
      "items": []
    },
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
