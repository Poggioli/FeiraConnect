{
  "request": {
    "method": "GET",
    "urlPathPattern": "/cities/(.+)",
    "headers": {
      "Cities-Slug": {
        "equalTo": "error-generic"
      }
    }
  },
  "response": {
    "fixedDelayMilliseconds": 2000,
    "status": 500,
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
